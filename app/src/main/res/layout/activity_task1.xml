<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#efefef"
    tools:context=".ActivityTask1">

    <FrameLayout
        android:id="@+id/frame1"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/gray"
        app:layout_constraintEnd_toStartOf="@+id/frame2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="@color/red" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="@color/white" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="@color/red" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame2"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame1"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="54dp" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="54dp"
                android:background="@color/red" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame3"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/white"
        app:layout_constraintEnd_toStartOf="@+id/frame4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="56dp"
                android:layout_height="match_parent"
                android:background="@color/green" />

            <FrameLayout
                android:layout_width="56dp"
                android:layout_height="match_parent" />

            <FrameLayout
                android:layout_width="56dp"
                android:layout_height="match_parent"
                android:background="@color/red" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame4"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame3"
        app:layout_constraintTop_toBottomOf="@+id/frame2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="54dp"
                android:background="@color/yellow" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:background="@color/blue" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:background="@color/red" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame5"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/white"
        app:layout_constraintEnd_toStartOf="@+id/frame6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="56dp"
                android:layout_height="match_parent" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="@color/red" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:background="@color/green" />

            </LinearLayout>

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame6"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame5"
        app:layout_constraintTop_toBottomOf="@+id/frame4">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="18dp"
                android:background="@color/red" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="18dp"
                android:background="@color/white" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="@color/dark_blue" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="18dp"
                android:background="@color/white" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="18dp"
                android:background="#ff0000" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame7"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/white"
        app:layout_constraintEnd_toStartOf="@+id/frame8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame5">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:id="@+id/frameLayout"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:background="@color/red"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <FrameLayout
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:background="@color/red"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="45dp"
                android:background="@color/red"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="45dp"
                android:background="@color/red"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame8"
        android:layout_width="168dp"
        android:layout_height="108dp"
        android:layout_margin="10dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frame7"
        app:layout_constraintTop_toBottomOf="@+id/frame6">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:id="@+id/frameLayout4"
                android:layout_width="match_parent"
                android:layout_height="9dp"
                android:background="@color/red"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <FrameLayout
                android:id="@+id/frameLayout6"
                android:layout_width="match_parent"
                android:layout_height="9dp"
                android:background="@color/red"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <FrameLayout
                android:id="@+id/frameLayout3"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:background="@color/red"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <FrameLayout
                android:id="@+id/frameLayout7"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:background="@color/red"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <FrameLayout
                android:id="@+id/frameLayout5"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@color/red"
                app:layout_constraintStart_toEndOf="@+id/frameLayout3"
                app:layout_constraintTop_toBottomOf="@+id/frameLayout4" />

            <FrameLayout
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@color/red"
                app:layout_constraintBottom_toTopOf="@+id/frameLayout6"
                app:layout_constraintStart_toEndOf="@+id/frameLayout3" />

            <FrameLayout
                android:id="@+id/frameLayout8"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@color/red"
                app:layout_constraintBottom_toTopOf="@+id/frameLayout6"
                app:layout_constraintEnd_toStartOf="@+id/frameLayout7" />

            <FrameLayout
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@color/red"
                app:layout_constraintEnd_toStartOf="@+id/frameLayout7"
                app:layout_constraintTop_toBottomOf="@+id/frameLayout4" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>